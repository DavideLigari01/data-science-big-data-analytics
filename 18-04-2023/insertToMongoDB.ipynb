    "import pymongo\n",
    "from pymongo import MongoClient\n",
    "import pandas as pd\n",
    "data=pd.read_json('dataset/restaurants.json',lines=True)\n",
    "\n",
    "conn = MongoClient()\n",
    "mydb = conn[\"restaurants\"]\n",
    "mycol = mydb[\"details\"]\n",
    "x = mycol.insert_many(data.to_dict('records'))\n",
    "x\n"
   ]
  }
